<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/navigation_home">
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.capstone.gometry.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_play_ar"
        android:name="com.capstone.gometry.ui.play_ar.PlayARFragment"
        android:label="PlayARFragment"
        tools:layout="@layout/fragment_play_ar"/>
    <fragment
        android:id="@+id/navigation_exam"
        android:name="com.capstone.gometry.ui.exam_list.ExamListFragment"
        android:label="fragment_exam_list"
        tools:layout="@layout/fragment_exam_list" />
    <fragment
        android:id="@+id/navigation_leaderboard"
        android:name="com.capstone.gometry.ui.leaderboard.LeaderboardFragment"
        android:label="fragment_leaderboard"
        tools:layout="@layout/fragment_leaderboard" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.capstone.gometry.ui.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>